{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "SEEDR_EMAIL": "@seedr_email",
        "SEEDR_PASSWORD": "@seedr_password"
    }
}
